<!DOCTYPE html>
<html>
<head>
  <% include ../../includes/header.ejs %>
  <link rel="stylesheet" href="/stylesheets/prove02style.css">
</head>

<body>
  <% include ../../includes/nav.ejs %>

  <% gpus.forEach (gpu => { %>
    <div class="row">
        <div class="col-m">
          <div class="jumbotron jumbotron-fluid itemContainer">
            <div class="container">
              <h1 class="display-4"><%= gpu.name %></h1>
              <img src="<%= gpu.url %>" style='max-width: 150px'>
              <p>$<%= gpu.price %></p>
            </div>
          </div>
        </div>
      </div>
      </div>
 <% }) %>
</body>
</html>
